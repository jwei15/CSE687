<Window x:Class="NavigatorClient.SelectedFilesWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NavigatorClient"
        mc:Ignorable="d"
        Title="SelectedFilesWindow" Height="450" Width="800">
    <Grid>
        <Grid Margin="5">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="0*"/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            <ListBox x:Name="SelectedFiles" FontFamily="tahoma" Grid.ColumnSpan="2"></ListBox>
        </Grid>
    </Grid>
</Window>


